import numpy as np
import numpy.ma as ma

d= np.array([3,3,3,0])
# masked = ma.masked_equal(d,0)
mask = d < 3
print(mask)

r=d[mask]
print(r)

# print(d)
# print(masked.mean())






marks = np.array([100,4,60,46,36,78,66,43,67])
failed =  marks[marks < 40]
print("failed",failed)

mask=marks > 10

marks[mask]=0
print(marks)

# fancy indexing

x = np.arange(1,11)
print(x)

y = np.array([3,4,5])
print(x[y])
